  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run forever:start",
    "stop": "npm run forever:stop",
    "forever:start": "TZ='Asia/Tokyo' NODE_ENV='production' forever -a start --uid 'blog' -c 'node --use_strict' --spinSleepTime 1 --minUptime 1000 ./src/app.js",
    "forever:stop": "forever stop blog && forever cleanlogs",
    "dump": "mongodump --db blog --out data/mongodump",
    "restore": "mongorestore --drop --db blog data/mongodump/blog/",
    "nginx:restart": "cp blog.conf /etc/nginx/conf.d/ && nginx -s reload",
    "new": "gulp posts:new",
    "edit": "gulp posts:edit -f",
    "upsert": "gulp posts:upsert",
    "pub": "gulp pub",
    "build": "gulp build",
